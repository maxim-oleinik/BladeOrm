{
  "name": "blade/orm",
  "type": "library",
  "description": "PHP ORM library",
  "license": "MIT",
  "authors": [{
    "name": "Maxim Oleinik",
    "email": "maxim.oleinik@gmail.com"
  }],
  "require": {
    "php": ">=7.0",
    "blade/database": "^0.2.0@dev"
  },
  "repositories": {
    "blade/database": {
      "type": "git",
      "url": "https://github.com/maxim-oleinik/blade-database.git"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "5.*"
  },
  "autoload": {
    "psr-4": {
